{
  "manifest_version": 3,
  "name": "Skip It",
  "version": "1.0.0",
  "description": "Skip it",
  "permissions": [],
  "background": {
    "service_worker": "./background/index.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/*"],
      "js": ["content/main.js"],
      "css": ["content/style/style.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://netflix.com/*", "http://netflix.com/*"],
      "resources": []
    }
  ],
  "action": {
    "default_title": "Skip it",
    "default_popup": "./popup/index.html"
  }
}
